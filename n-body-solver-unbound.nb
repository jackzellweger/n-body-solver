Cell[BoxData[
 RowBox[{
  RowBox[{"equationsOfMotionX", "=", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{
      RowBox[{
       SuperscriptBox[
        RowBox[{"x", "[", "j", "]"}], "\[Prime]\[Prime]"], "[", "t", "]"}], 
      "\[Equal]", 
      RowBox[{
       UnderoverscriptBox["\[Sum]", 
        RowBox[{"i", "=", "1"}], "planetNumber"], 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{"i", "\[Equal]", "j"}], ",", "0", ",", 
         RowBox[{"(", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{"G", " ", 
             RowBox[{
             "m", "\[LeftDoubleBracket]", "j", "\[RightDoubleBracket]"}], " ", 
             RowBox[{"(", 
              RowBox[{
               RowBox[{
                RowBox[{"x", "[", "i", "]"}], "[", "t", "]"}], "-", 
               RowBox[{
                RowBox[{"x", "[", "j", "]"}], "[", "t", "]"}]}], ")"}]}], 
            ")"}], "/", 
           RowBox[{"(", 
            RowBox[{
             SqrtBox[
              RowBox[{
               SuperscriptBox[
                RowBox[{"(", 
                 RowBox[{
                  RowBox[{
                   RowBox[{"x", "[", "i", "]"}], "[", "t", "]"}], "-", 
                  RowBox[{
                   RowBox[{"x", "[", "j", "]"}], "[", "t", "]"}]}], ")"}], 
                "2"], "+", 
               SuperscriptBox[
                RowBox[{"(", 
                 RowBox[{
                  RowBox[{
                   RowBox[{"y", "[", "i", "]"}], "[", "t", "]"}], "-", 
                  RowBox[{
                   RowBox[{"y", "[", "j", "]"}], "[", "t", "]"}]}], ")"}], 
                "2"]}]], " ", 
             RowBox[{"(", 
              RowBox[{
               SuperscriptBox[
                RowBox[{"(", 
                 RowBox[{
                  RowBox[{
                   RowBox[{"x", "[", "i", "]"}], "[", "t", "]"}], "-", 
                  RowBox[{
                   RowBox[{"x", "[", "j", "]"}], "[", "t", "]"}]}], ")"}], 
                "2"], "+", 
               SuperscriptBox[
                RowBox[{"(", 
                 RowBox[{
                  RowBox[{
                   RowBox[{"y", "[", "i", "]"}], "[", "t", "]"}], "-", 
                  RowBox[{
                   RowBox[{"y", "[", "j", "]"}], "[", "t", "]"}]}], ")"}], 
                "2"]}], ")"}]}], ")"}]}], ")"}]}], "]"}]}]}], ",", 
     RowBox[{"{", 
      RowBox[{"j", ",", "1", ",", "planetNumber"}], "}"}]}], "]"}]}], 
